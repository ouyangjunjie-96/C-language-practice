#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
    int a[6]={9,4,2,5,6,-1};
    sort(a,a+4);
    for(int i=0;i<6;i++)
    {
        printf("%d ",a[i]);
    }
    printf("\n");
    sort(a,a+6);
    for(int i=0;i<6;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
    double a[]={1.4,-2.1,9};
    sort(a,a+3);
    for(int i=0;i<3;i++)
    {
        printf("%.1f",a[i]);
    }
    return 0;
}
