-------------------swap--------------------------------
#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
     int x=1,y=-2;
     swap(x,y);
     printf("%d %d\n",x,y);
     return 0;
}
-------------------reverse----------------------------
#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
    int a[10]={10,11,12,13,14,15};
    reverse(a,a+4);
    for(int i=0;i<6;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
#include<stdio.h>
#include<string>
#include<algorithm>
using namespace std;
int main()
{
    string str="abcdefghi";
    reverse(str.begin()+2,str.begin()+6);
    for(int i=0;i<str.length;i++)
    {
        printf("%c",str[i]);
    }
    return 0;
}
---------------------------next_permutation--------------------------
#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
    int a[10]={1,2,3};
    do
    {
         printf("%d%d%d%d\n",a[0],a[1],a[2]);
    }
    while(next_permutation(a,a+3));
    return 0;
}
#include<stdio.h>
#include<algorithm>
using namespace std;
int main()
{
    int a[5]={1,2,3,4,5};
    fill(a,a+5,233);
    for(int i=0;i<5;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
